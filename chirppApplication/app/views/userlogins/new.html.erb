  <h1>Login </h1>

<%= form_tag("/userlogins", :remote => true) do %>
    <p><%= label_tag(:username, "Username:") %>
    <%= text_field_tag(:username) %><br><br></p>
    <p><%= label_tag(:password, "Password:") %>
    <%= password_field_tag(:password) %><br><br>
    <%= submit_tag("Login") %></p>
<% end %> 
