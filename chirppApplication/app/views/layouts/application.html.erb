<html>
<head>
  <title>ChirppApplication</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="wrapper">
    <div id="headerwrap">
      <div id="header">
       <%= render "layouts/header" %>
      </div>
    </div>
    <br>
    <div id="contentliquid"><div id="contentwrap">
      <div id="content">
          <%= yield %>
      </div>
    </div></div>
    <div id="leftcolumnwrap">
      <div id="leftcolumn">
          <%= render "layouts/nav" %>
      </div>
    </div> 
     <div id="rightcolumnwrap">
        <div id="rightcolumn">
        <%= render "chirpps/show_all" %>
      </div>
    </div>

   <div id="footerwrap">
      <div id="footer">
        <%= render "layouts/footer" %>
      </div>
  </div>
    
  </div>
</body>
</html>